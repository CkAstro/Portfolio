{
   "name": "fullstack-template-server",
   "version": "0.1.0",
   "description": "full stack react/ts express package",
   "author": "Chris Kolb",
   "type": "module",
   "scripts": {
      "start": "npm run dev",
      "dev": "webpack --node-env development --config webpack.config.cjs --watch",
      "release": "npm run lint && npm run format:check && npm run ts-check && npm run spellcheck && npm run test && npm run build",
      "build": "npm run build:production",
      "build:production": "npm run clean:dist && webpack --node-env production --config webpack.config.cjs",
      "build:development": "npm run clean:dist && webpack --node-env development --config webpack.config.cjs",
      "test": "test/run-jest.sh",
      "test:commit": "jest --clearCache && jest --watchAll=false",
      "list-outdated-dependencies": "npm-check-updates --format repo,group --peer",
      "update-dependencies": "npm run list-outdated-dependencies -- -u && npm install && npm update && npm run lint:fix && npm run format && npm run release",
      "clean": "rm -rf node_modules coverage dist",
      "clean:dist": "rm -rf dist"
   },
   "dependencies": {
      "@reduxjs/toolkit": "^1.9.5",
      "axios": "^1.4.0",
      "compression": "^1.7.4",
      "cors": "^2.8.5",
      "dotenv": "^16.1.4",
      "nodemailer": "^6.9.3",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "react-draggable": "^4.4.6",
      "react-redux": "^8.0.7",
      "react-router-dom": "^6.11.1",
      "redux-thunk": "^2.4.2"
   },
   "devDependencies": {
      "@babel/core": "^7.21.8",
      "@babel/eslint-parser": "^7.21.8",
      "@babel/preset-env": "^7.21.5",
      "@babel/preset-react": "^7.18.6",
      "@babel/preset-typescript": "^7.21.5",
      "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
      "@testing-library/jest-dom": "^5.16.5",
      "@testing-library/react": "^14.0.0",
      "@testing-library/user-event": "^14.4.3",
      "@types/compression": "^1.7.2",
      "@types/cors": "^2.8.13",
      "@types/jest": "^29.5.12",
      "@types/nodemailer": "^6.4.8",
      "@types/react": "^18.2.6",
      "@types/react-dom": "^18.2.4",
      "@types/react-redux": "^7.1.25",
      "@types/react-router-dom": "^5.3.3",
      "@typescript-eslint/eslint-plugin": "^5.59.5",
      "@typescript-eslint/parser": "^5.59.5",
      "babel-loader": "^9.1.2",
      "copy-webpack-plugin": "^11.0.0",
      "core-js": "^3.30.2",
      "cspell": "^6.31.1",
      "css-loader": "^6.7.3",
      "css-minimizer-webpack-plugin": "^5.0.0",
      "dotenv-webpack": "^8.0.1",
      "eslint": "^8.40.0",
      "eslint-plugin-import": "^2.27.5",
      "eslint-plugin-jest": "^27.2.1",
      "eslint-plugin-jsx-a11y": "^6.7.1",
      "eslint-plugin-react": "^7.32.2",
      "eslint-plugin-react-hooks": "^4.6.0",
      "eslint-plugin-testing-library": "^5.11.0",
      "file-loader": "^6.2.0",
      "fork-ts-checker-webpack-plugin": "^8.0.0",
      "html-minimizer-webpack-plugin": "^4.3.0",
      "html-webpack-plugin": "^5.5.1",
      "husky": "^8.0.3",
      "identity-obj-proxy": "^3.0.0",
      "jest": "^29.5.0",
      "jest-environment-jsdom": "^29.5.0",
      "lint-staged": "^13.2.2",
      "mini-css-extract-plugin": "^2.7.5",
      "mini-svg-data-uri": "^1.4.4",
      "nodemon": "^2.0.22",
      "nodemon-webpack-plugin": "^4.8.1",
      "npm-check-updates": "^16.10.12",
      "npm-run-all": "^4.1.5",
      "postcss": "^8.4.23",
      "postcss-loader": "^7.3.0",
      "postcss-preset-env": "^8.3.2",
      "prettier": "^2.8.8",
      "react-refresh": "^0.14.0",
      "sass": "^1.62.1",
      "sass-loader": "^13.2.2",
      "style-loader": "^3.3.2",
      "ts-jest": "^29.1.4",
      "ts-loader": "^9.4.3",
      "ts-node": "^10.9.1",
      "typescript": "^5.0.4",
      "url-loader": "^4.1.1",
      "webpack": "^5.82.1",
      "webpack-bundle-analyzer": "^4.8.0",
      "webpack-cli": "^5.1.1",
      "webpack-dev-server": "^4.15.0",
      "webpack-node-externals": "^3.0.0"
   },
   "engines": {
      "node": ">=16.8.0"
   }
}
